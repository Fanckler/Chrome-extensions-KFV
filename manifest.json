{
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "s.kava1997@gmail.com",
  "name": "Kava FloatVideo",
  "offline_enabled": true,
  "short_name": "KavaFloatVideo",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "description": "Всплывающее окно для просмотра видео с любой вкладки браузера.",
  "browser_action": {
    "default_title": "Kava FloatVideo"
  },
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  }
}
